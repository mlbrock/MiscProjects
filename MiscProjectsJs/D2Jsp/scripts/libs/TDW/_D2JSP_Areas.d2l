// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_GetNumberBasic(area_number)
{
	if (D2JSP_UTIL_GEN_TypeOfNumber(area_number))
		return(area_number);
	else if (D2JSP_UTIL_GEN_TypeOfString(area_number) &&
		D2JSP_UTIL_FORMAT_IsDigit(area_number))
		return(parseInt(area_number, 10));
	else
		return(me.area);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_IsValid(area_number)
{
	return(((area_number >= D2JSP_AREA_MINIMUM) &&
		(area_number <= D2JSP_AREA_MAXIMUM)) ? true : false);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_IsValidInGame(area_number)
{
	return(((area_number > D2JSP_AREA_MINIMUM) &&
		(area_number <= D2JSP_AREA_MAXIMUM)) ? true : false);
}
// ////////////////////////////////////////////////////////////////////////////

// NB: The Secret Cow Level is considered to be in Act 1 by this function.
// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_GetAct(area_number)
{
	area_number = D2JSP_AREA_GetNumberBasic(area_number);

	if (D2JSP_AREA_IsValidInGame(area_number))
		return((area_number < D2JSP_AREA_LUT_GHOLEIN) ? 1 :
			(area_number < D2JSP_AREA_KURAST_DOCKTOWN) ? 2 :
			(area_number < D2JSP_AREA_THE_PANDEMONIUM_FORTRESS) ? 3 :
			(area_number < D2JSP_AREA_HARROGATH) ? 4 : 5);
	else
		return(-1);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_HasWayPoint(area_number)
{
	var count_1;
	var count_2;

	area_number = D2JSP_AREA_GetNumberBasic(area_number);

	if (D2JSP_AREA_IsValidInGame(area_number)) {
		for (count_1 = 0; count_1 < D2JSP_WayPoints.length; count_1++) {
			for (count_2 = 0; count_2 < D2JSP_WayPoints[count_1].length;
				count_2++) {
				if (area_number == D2JSP_WayPoints[count_1][count_2])
					return(true);
			}
		}
	}

	return(false);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_AreaToWayPointIndices(area_number)
{
	var count_1;
	var count_2;

	area_number = D2JSP_AREA_GetNumberBasic(area_number);

	if (D2JSP_AREA_IsValidInGame(area_number)) {
		for (count_1 = 0; count_1 < D2JSP_WayPoints.length; count_1++) {
			for (count_2 = 0; count_2 < D2JSP_WayPoints[count_1].length;
				count_2++) {
				if (area_number == D2JSP_WayPoints[count_1][count_2])
					return([count_1, count_2]);
			}
		}
	}

	return(null);
}
