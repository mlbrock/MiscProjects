function D2JSP_AREA_Waypoint(area, room_spec, unit_id_spec)
{
	this.area         = area;
	this.room_spec    = D2JSP_UTIL_GEN_ExpandArgs(room_spec);
	this.unit_id_spec = D2JSP_UTIL_GEN_ExpandArgs(unit_id_spec);

	this.toString     = D2JSP_AREA_Waypoint_toString;

	function D2JSP_AREA_Waypoint_toString() {
		return("Waypoint for area " + this.area + " (" +
			D2JSP_AREA_AreaToName(this.area) + ") in room spec [" +
			this.room_spec + "], classid spec [" + this.unit_id_spec + "].");
	}
}
