// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_IHaveWayPointArea(area_number)
{
	area_number = D2JSP_AREA_GetNumberBasic(area_number);

	if (parseFloat(me.version) >= 0.32)
		return((D2JSP_AREA_IsValidInGame(area_number) &&
			getWaypoint(D2JSP_AreaToWPIndex[area_number])) ? true : false);
	else
		return(null);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_IHaveWayPointIndex(way_point_index)
{
	if (parseFloat(me.version) >= 0.32)
		return(((way_point_index >= 0) &&
			(way_point_index < D2JSP_WPIndexToArea.length) &&
			getWaypoint(way_point_index)) ? true : false);
	else
		return(null);
}
