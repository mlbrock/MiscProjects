function D2JSP_AREA_ActToTown(act_number)
{
	return((D2JSP_UTIL_GEN_TypeOfNumber(act_number) && (act_number >= 1) &&
		(act_number <= 5)) ? D2JSP_WayPoints[act_number - 1][0] : -1);
}
