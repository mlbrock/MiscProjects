// ////////////////////////////////////////////////////////////////////////////
function D2JSP_Area(area_number, area_name, area_name_mpq, is_inside, drlg_type,
	level_type, sub_type, is_complex)
{
	this.area_number   = area_number;
	this.area_name     = area_name;
	this.area_name_mpq = (area_name_mpq == null) ? area_name : area_name_mpq;
	this.is_inside     = (is_inside) ? true : false;
	this.drlg_type     = drlg_type;
	this.level_type    = level_type;
	this.sub_type      = sub_type;
	this.is_complex    = (is_complex)  ? true : false;
	this.is_outside    = (this.is_inside) ? false : true;

	this.act           = (area_number < D2JSP_AREA_LUT_GHOLEIN) ? 1 :
		(area_number < D2JSP_AREA_KURAST_DOCKTOWN) ? 2 :
		(area_number < D2JSP_AREA_THE_PANDEMONIUM_FORTRESS) ? 3 :
		(area_number < D2JSP_AREA_HARROGATH) ? 4 : 5;
	this.is_act_first  = ((area_number == D2JSP_AREA_ROGUE_ENCAMPMENT) ||
		(area_number == D2JSP_AREA_LUT_GHOLEIN) ||
		(area_number == D2JSP_AREA_KURAST_DOCKTOWN) ||
		(area_number == D2JSP_AREA_THE_PANDEMONIUM_FORTRESS) ||
		(area_number == D2JSP_AREA_HARROGATH)) ? true : false;
	this.is_act_last   = ((area_number == D2JSP_AREA_CATACOMBS_LEVEL_4) ||
		(area_number == D2JSP_AREA_DURIELS_LAIR) ||
		(area_number == D2JSP_AREA_DURANCE_OF_HATE_LEVEL_3) ||
		(area_number == D2JSP_AREA_CHAOS_SANCTUM) ||
		(area_number == D2JSP_AREA_THE_WORLDSTONE_CHAMBER)) ? true : false;
	this.is_town       = this.is_act_first;

	this.has_wp          = false;
	this.is_act_wp_first = false;
	this.is_act_wp_last  = false;
	this.wp_index_major  = 0;
	this.wp_index_minor  = 0;

	for (var count_1 = 0; count_1 < D2JSP_WayPoints[this.act - 1].length;
		count_1++) {
		if (area_number == D2JSP_WayPoints[this.act - 1][count_1]) {
			this.has_wp          = true;
			this.is_act_wp_first = this.is_act_first;
			this.is_act_wp_last  =
				((count_1 + 1) == D2JSP_WayPoints[this.act - 1].length) ?
				true : false;
			this.wp_index_major  = this.act - 1;
			this.wp_index_minor  = count_1;
			break;
		}
	}
}
