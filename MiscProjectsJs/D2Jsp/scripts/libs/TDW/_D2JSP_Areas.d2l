function D2JSP_AREA_NameToArea(area_name)
{
	var count_1;
	var this_name;

	area_name.toLowerCase();

	for (count_1 = 0; count_1 < D2JSP_AREA_AreaNameList.length; count_1++) {
		this_name = D2JSP_AREA_AreaNameList[count_1];
		if (area_name == this_name.toLowerCase())
			return(count_1);
	}

