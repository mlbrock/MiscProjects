function D2JSP_AREA_AreaToTown(area_number)
{
	area_number = D2JSP_AREA_GetNumberBasic(area_number);

	return((D2JSP_AREA_IsValidInGame(area_number)) ?
		D2JSP_WayPoints[D2JSP_AREA_GetAct(area_number) - 1][0] : "*INVALID*");
}
