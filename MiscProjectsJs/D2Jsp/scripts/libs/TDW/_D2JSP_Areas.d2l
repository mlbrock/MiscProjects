// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_GetWaypointObjectForArea(area_number)
{
	var waypoint_indices = D2JSP_AREA_AreaToWayPointIndices(area_number);

	return((!waypoint_indices) ? null :
		D2JSP_AREA_WaypointObjList[waypoint_indices[0]][waypoint_indices[1]]);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_GetAreaTransitTile(area_number)
{
	area_number = D2JSP_AREA_GetNumberBasic(area_number);

	if (D2JSP_AREA_IsValid(area_number)) {
		var tile_obj = getUnit(D2JSP_UTIL_GETUNIT_TILE);
		if (tile_obj) {
			do {
				if (tile_obj.area == area_number)
					return(tile_obj);
			} while (tile_obj && tile_obj.getNext());
		}
	}

	return(null);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_IsValidWaypointAccess(wp_access)
{
	if (D2JSP_UTIL_GEN_TypeOfArrayIndexed(wp_access) &&
		(wp_access.length == D2JSP_AREA_WaypointObjList.length)) {
		for (var count_1 = 0; count_1 < wp_access.length; count_1++) {
			if (wp_access[count_1].length !=
				D2JSP_AREA_WaypointObjList[count_1].length)
				return(false);
			for (var count_2 = 0; count_2 < wp_access[count_1].length; count_2++) {
				if ((!D2JSP_UTIL_GEN_TypeOfBoolean(wp_access[count_1][count_2])) &&
					(!D2JSP_UTIL_GEN_TypeOfNumber(wp_access[count_1][count_2])))
					return(false);
			}
		}
		return(true);
	}

	return(false);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_HasWaypointAccess(area_number, wp_access)
{
	if (D2JSP_AREA_IsValidWaypointAccess(wp_access) &&
		D2JSP_AREA_IsValidInGame(area_number) &&
		D2JSP_AREA_HasWayPoint(area_number)) {
		var wp_indices = D2JSP_AREA_AreaToWayPointIndices(area_number);
		return(wp_access[wp_indices[0]][wp_indices[1]])
	}

	return(false);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_GetEmptyWaypointAccess()
{
	var wp_access = D2JSP_UTIL_GEN_CloneObject(D2JSP_WayPoints);

	for (var count_1 = 0; count_1 < wp_access.length; count_1++) {
		for (var count_2 = 0; count_2 < wp_access[count_1].length; count_2++)
			wp_access[count_1][count_2] = false;
	}

	return(wp_access);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_GetAreaCoords(area_number)
{
	area_number = D2JSP_AREA_GetNumberBasic(area_number);

	if (D2JSP_AREA_IsValidInGame(area_number) &&
		(D2JSP_AREA_GetAct(area_number) == me.act)) {
		var room_obj = getRoom(area_number);
		if (room_obj) {
			var x_1 = room_obj.levelx;
			var y_1 = room_obj.levely;
			var x_2 = 0;
			var y_2 = 0;
			do {
				var coll_table = room_obj.getCollision();
				x_2 = Math.max(x_2, (room_obj.x * 5) + (coll_table[0].length - 1));
				y_2 = Math.max(y_2, (room_obj.y * 5) + (coll_table.length - 1));
			} while (room_obj && room_obj.getNext(area_number));
			return([x_1, y_1, x_2, y_2]);
		}
	}

	return(null);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_IsInAreaXYWithCoords(area_coords, in_x, in_y)
{
/*
print("D2JSP_AREA_IsInAreaXYWithCoords([" + area_coords + "], " + in_x +
	", " + in_y + ")");
*/
	return((area_coords && (in_x >= area_coords[0]) &&
		(in_x <= area_coords[2]) && (in_y >= area_coords[1]) &&
		(in_y <= area_coords[3])) ? true : false);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_IsInAreaXY(area_number, in_x, in_y)
{
	return(D2JSP_AREA_IsInAreaXYWithCoords(D2JSP_AREA_GetAreaCoords(area_number),
		in_x, in_y));
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_IsInAreaPair(area_number, in_pair)
{
	return(D2JSP_AREA_IsInAreaXY(area_number, in_pair[0], in_pair[1]));
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_AREA_IsInAreaObject(area_number, in_object)
{
	return(D2JSP_AREA_IsInAreaXY(area_number, in_object.x, in_object.y));
}
