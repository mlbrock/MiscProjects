// ////////////////////////////////////////////////////////////////////////////
function D2JSP_STAT_Get(in_obj, stat_number)
{
	var stat_value = null;

	if (D2JSP_STAT_IsValid(stat_number)) {
		stat_value = in_obj.getStat(stat_number);
		if (parseFloat(me.version) >= 0.3527) {
			if (stat_number == D2JSP_STAT_ITEM_ARMOR_PERCENT)
				stat_value = in_obj.getStatEx(stat_number);
			else if (stat_number == D2JSP_STAT_ITEM_MAXDURABILITY_PERCENT)
				stat_value = in_obj.getStatEx(stat_number);
			// Don't want to check to see if it's a weapon...
			else if (((stat_number == D2JSP_STAT_ITEM_MAXDAMAGE_PERCENT) ||
				(stat_number == D2JSP_STAT_ITEM_MINDAMAGE_PERCENT)) &&
				(!stat_value))
				stat_value = in_obj.getStatEx(stat_number);
			else if ((stat_number == D2JSP_STAT_ITEM_ARMOR_PERCENT) &&
				(!stat_value))
				stat_value = in_obj.getStatEx(stat_number);
		}
	}

	return(stat_value);	
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_STAT_IsValid(stat_number)
{
	return(((stat_number >= D2JSP_STAT_MINIMUM) &&
		(stat_number <= D2JSP_STAT_MAXIMUM)) ? true : false);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_STAT_IsName(stat_name)
{
	return((typeof(D2JSP_STAT_NameToStatList[stat_name.toLowerCase()]) !=
		"undefined") ? true : false);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_STAT_StatToName(stat_number)
{
	return((D2JSP_STAT_IsValid(stat_number)) ?
		D2JSP_STAT_StatNameList[stat_number] : "*INVALID*");
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_STAT_NameToStat(stat_name)
{
	return((D2JSP_STAT_IsName(stat_name)) ?
		D2JSP_STAT_NameToStatList[stat_name.toLowerCase()] : -1);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_STAT_ParseToNumber(in_stat)
{
	var tmp_number;
	var tmp_data;

	in_stat  = D2JSP_UTIL_FORMAT_Trim(in_stat + "");
	tmp_data = in_stat;
	tmp_data = tmp_data.replace(/^\s+|\s+$/g, "");
	tmp_data = tmp_data.replace(/\s+/g, "_");
	tmp_data = tmp_data.replace(/^[^A-Za-z_]|[^A-Za-z0-9_]/g, "");
	tmp_data = (tmp_data != "") ? tmp_data : "\"\"";

	if (D2JSP_UTIL_FORMAT_IsDigit(in_stat)) {
		tmp_number = parseInt(in_stat, 10);
		return((D2JSP_STAT_IsValid(tmp_number)) ? tmp_number : -1);
	}
	else if (eval("((typeof(D2JSP_STAT_" + tmp_data.toUpperCase() +
		") == \"number\") ? true : false)"))
		return(eval("D2JSP_STAT_" + tmp_data.toUpperCase()));
	else if (eval("((typeof(" + tmp_data.toUpperCase() +
		") == \"number\") ? true : false)"))
		return(eval(tmp_data.toUpperCase()));
	else if (D2JSP_STAT_IsName(in_stat))
		return(D2JSP_STAT_NameToStat(in_stat));

	return(-1);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_STAT_ParseToName(in_stat)
{
	var tmp_number = D2JSP_STAT_ParseToNumber(in_stat);

	return((tmp_number != -1) ? D2JSP_STAT_StatToName(tmp_number) : null);
}
