// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_ENCHANT_IsValid(enchant_number)
{
	return(((enchant_number >= D2JSP_ENCHANT_MINIMUM) &&
		(enchant_number <= D2JSP_ENCHANT_MAXIMUM) &&
		(typeof(D2JSP_ENCHANT_NameList[enchant_number]) == "string")) ?
		true : false);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_ENCHANT_IsName(enchant_name)
{
	return((typeof(D2JSP_ENCHANT_NameLookupList[enchant_name.toLowerCase()]) !=
		"undefined") ? true : false);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_ENCHANT_EnchantToName(enchant_number)
{
	return((D2JSP_ENCHANT_IsValid(enchant_number)) ?
		D2JSP_ENCHANT_NameList[enchant_number] : "*INVALID*");
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_ENCHANT_NameToEnchant(enchant_name)
{
	return((D2JSP_ENCHANT_IsName(enchant_name)) ?
		D2JSP_ENCHANT_NameLookupList[enchant_name.toLowerCase()] : -1);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_ENCHANT_ParseToNumber(in_data)
{
	var tmp_number;
	var tmp_data;

	in_data  = D2JSP_UTIL_FORMAT_Trim(in_data + "");
	tmp_data = in_data;
	tmp_data = tmp_data.replace(/^\s+|\s+$/g, "");
	tmp_data = tmp_data.replace(/\s+/g, "_");
	tmp_data = tmp_data.replace(/^[^A-Za-z_]|[^A-Za-z0-9_]/g, "");
	tmp_data = (tmp_data != "") ? tmp_data : "\"\"";

	if (D2JSP_UTIL_FORMAT_IsDigit(in_data)) {
		tmp_number = parseInt(in_data, 10);
		return((D2JSP_ENCHANT_IsValid(tmp_number)) ? tmp_number : -1);
	}
	else if (eval("((typeof(D2JSP_ENCHANT_" + tmp_data.toUpperCase() +
		") == \"number\") ? true : false)"))
		return(eval("D2JSP_ENCHANT_" + tmp_data.toUpperCase()));
	else if (eval("((typeof(" + tmp_data.toUpperCase() +
		") == \"number\") ? true : false)"))
		return(eval(tmp_data.toUpperCase()));
	else if (D2JSP_ENCHANT_IsName(in_data))
		return(D2JSP_ENCHANT_NameToEnchant(in_data));

	return(-1);
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function D2JSP_ENCHANT_ParseToName(in_data)
{
	var tmp_number = D2JSP_ENCHANT_ParseToNumber(in_data);

	return((tmp_number != -1) ? D2JSP_ENCHANT_EnchantToName(tmp_number) : null);
}
