// ////////////////////////////////////////////////////////////////////////////
function main()
{
	eval("DoAtInterval(3000);");

	print("Started eval()");
}
// ////////////////////////////////////////////////////////////////////////////

// ////////////////////////////////////////////////////////////////////////////
function DoAtInterval(interval)
{
	while (true) {
		var expiry = getTickCount() + interval;
		while (getTickCount() < expiry)
			delay(100);
		print("Interval done!");
	}
}
// ////////////////////////////////////////////////////////////////////////////

